{
  "extends": ["airbnb-base", "prettier"],
  "parser": "babel-eslint",
  "plugins": ["prettier"],
  "rules": {
    "global-require": 0,
    "prettier/prettier": ["error"],
    "no-restricted-globals": 0
  },
  "globals": {
    "__html__": "readonly",
    "document": "readonly",
    "parent": "writable",
    "window": "readonly"
  }
}
